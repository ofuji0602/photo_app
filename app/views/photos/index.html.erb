<h1>写真一覧</h1>

<% @photos.each do |photo| %>
  <div class="photo">
    <h2><%= photo.title %></h2>
    <% if photo.image.attached? %>
      <%= image_tag photo.image.variant(resize_to_limit: [300, 300]).processed %>
    <% else %>
      <p>画像はありません</p>
    <% end %>
  </div>
<% end %>
<%= link_to '戻る', root_path %>
<%= link_to '新しい写真を投稿する', new_photo_path %>
