<h1>Upload a Photo</h1>

<%= form_with(model: @photo, local: true) do |form| %>
  <div>
    <%= form.label :title %>
    <%= form.text_field :title %>
  </div>

  <div>
    <%= form.label :image %>
    <%= form.file_field :image %>
  </div>

  <div>
    <%= form.submit 'Upload Photo' %>
  </div>
<% end %>

<%= link_to 'Back', root_path %>
